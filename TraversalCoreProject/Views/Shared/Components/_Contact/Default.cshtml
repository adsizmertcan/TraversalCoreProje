@using EntityLayer.Concrete
@model List<Contact>

@foreach (var item in Model)
{
<div class="col-lg-6 contact-left">
    <div class="partners">
        <div class="cont-details">
            <h5>İletişim Bilgilerimiz</h5>
            <p class="mt-3 mb-4">
                @item.ContactDesc
            </p>
        </div>
        <div class="hours">
            <h6 class="mt-4">Mail Adresimiz:</h6>
            <p>@item.ContactMail</p>
            <h6 class="mt-4">Ofisimize Gelin:</h6>
            <p> @item.ContactAddress</p>
            <h6 class="mt-4">Telefon Numarası:</h6>
            <p class="margin-top">@item.ContactPhone</p>
        </div>
    </div>
</div>
}